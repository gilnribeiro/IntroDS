       "10455423.777777778"
      ]
     },
     "execution_count": 120,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "#type your code here\n",
    "portugal_value_series = df.loc[(df['Series Name'] == 'Population, total') & \n",
    "       (df['Country Name'] == 'Portugal'), 'value']\n",
    "mean_gdp_port = portugal_value_series.mean()\n",
    "mean_gdp_port"
   ]
